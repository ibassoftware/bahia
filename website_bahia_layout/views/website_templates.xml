<openerp>
    <data>

        <template id="layout_new_logo" inherit_id="website.layout" name="Change the Website Main Logo">
            <xpath expr="//div[@class='box-container']/div[@class='top_animation']" position="attributes">

                <attribute name="style">height:334px;max-height:334px</attribute>
            </xpath>            

            <xpath expr="//div[@class='box-container']/div[@class='top_animation']/object[@classid='clsid:d27cdb6e-ae6d-11cf-96b8-444553540000']" position="replace">
                <img src="/website/static/src/img/bahia/bahia_banner_2.jpg" style="width:962px;height:344px"/>
            </xpath>            
        </template>
     </data>
</openerp>
